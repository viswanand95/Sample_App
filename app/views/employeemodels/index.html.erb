<p id="notice"><%= notice %></p>

<h1>Employeemodels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Designation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employeemodels.each do |employeemodel| %>
      <tr>
        <td><%= employeemodel.name %></td>
        <td><%= employeemodel.age %></td>
        <td><%= employeemodel.gender %></td>
        <td><%= employeemodel.designation %></td>
        <td><%= link_to 'Show', employeemodel %></td>
        <td><%= link_to 'Edit', edit_employeemodel_path(employeemodel) %></td>
        <td><%= link_to 'Destroy', employeemodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employeemodel', new_employeemodel_path %>
